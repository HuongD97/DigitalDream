<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="me.huongdang.digitaldream.SetBudgets">

    <Button
        android:id="@+id/button2"
        android:layout_width="80dp"
        android:layout_height="47dp"
        android:layout_marginLeft="18dp"
        android:text="@string/budget_name"
        app:layout_constraintLeft_toRightOf="@+id/progressBar"
        tools:layout_editor_absoluteY="99dp" />

    <ProgressBar
        android:id="@+id/progressBar3"
        style="@style/Widget.AppCompat.ProgressBar.Horizontal"
        android:layout_width="259dp"
        android:layout_height="78dp"
        android:indeterminateTint="@color/colorPrimary"
        android:progress="@android:integer/config_shortAnimTime"
        android:progressTint="@color/colorPrimaryDark"
        android:visibility="visible"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="317dp"
        tools:visibility="visible" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Progress"
        tools:layout_editor_absoluteX="19dp"
        tools:layout_editor_absoluteY="331dp" />

    <ProgressBar
        android:id="@+id/progressBar2"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="253dp"
        android:layout_height="111dp"
        android:indeterminateTint="@color/colorPrimaryDark"
        android:progress="@android:integer/config_mediumAnimTime"
        android:progressTint="@color/colorPrimaryDark"
        android:visibility="visible"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="67dp"
        tools:visibility="visible" />

    <ProgressBar
        android:id="@+id/progressBar"
        style="@style/Widget.AppCompat.ProgressBar.Horizontal"
        android:layout_width="256dp"
        android:layout_height="91dp"
        android:indeterminateTint="@color/colorPrimaryDark"
        android:progress="@android:integer/config_mediumAnimTime"
        android:progressTint="@color/colorPrimaryDark"
        android:visibility="visible"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="186dp"
        tools:visibility="visible" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Progress"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="211dp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="54dp"
        android:layout_height="55dp"
        app:srcCompat="@mipmap/budgeticon_grocery"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="44dp" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="56dp"
        android:layout_height="55dp"
        app:srcCompat="@mipmap/ic_launcher"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="156dp" />

    <Button
        android:id="@+id/button3"
        android:layout_width="80dp"
        android:layout_height="47dp"
        android:text="@string/budget_name"
        tools:layout_editor_absoluteY="211dp"
        app:layout_constraintLeft_toRightOf="@+id/progressBar"
        android:layout_marginLeft="18dp" />

    <Button
        android:id="@+id/button4"
        android:layout_width="81dp"
        android:layout_height="0dp"
        android:text="@string/budget_name"
        tools:layout_editor_absoluteX="287dp"
        tools:layout_editor_absoluteY="331dp" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="59dp"
        android:layout_height="59dp"
        app:srcCompat="@mipmap/car_icon"
        tools:layout_editor_absoluteY="272dp"
        tools:layout_editor_absoluteX="16dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Progress"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="99dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="121dp"
        android:layout_height="32dp"
        android:text="FOOD"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        tools:layout_editor_absoluteX="70dp"
        tools:layout_editor_absoluteY="57dp" />

</android.support.constraint.ConstraintLayout>
